{
  "rabbit_version": "3.11.19",
  "rabbitmq_version": "3.11.19",
  "product_name": "RabbitMQ",
  "product_version": "3.11.19",
  "users": [
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "guest",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "topic_permissions": [],
  "parameters": [
    {
      "value": {
        "max-hops": 1,
        "prefetch-count": 200,
        "reconnect-delay": 10,
        "trust-user-id": true,
        "uri": "amqp://haproxy-odc:5672/%2F?heartbeat=45&connection_timeout=15000"
      },
      "vhost": "/",
      "component": "federation-upstream",
      "name": "fed-omaha"
    }
  ],
  "global_parameters": [
  ],
  "policies": [
    {
      "vhost": "/",
      "name": "quorumfedqueues",
      "pattern": ".*-fed$",
      "apply-to": "queues",
      "definition": {
        "federation-upstream-set": "all",
        "max-length": 1500000,
        "max-length-bytes": 1050000000,
        "queue-leader-locator": "balanced"
      },
      "priority": 25
    }
  ],
  "queues": [
    {
      "name": "inventory-fed",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-dead-letter-exchange": "inventory-dlq",
        "x-message-ttl": 86400000,
        "x-queue-type": "quorum"
      }
    },
    {
      "name": "inventory-dlq",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-overflow": "drop-head",
        "x-queue-type": "quorum"
      }
    }
  ],
  "exchanges": [
    {
      "name": "inventory",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "inventory-dlq",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "inventory",
      "vhost": "/",
      "destination": "inventory-fed",
      "destination_type": "queue",
      "routing_key": "#",
      "arguments": {}
    },
    {
      "source": "inventory-dlq",
      "vhost": "/",
      "destination": "inventory-dlq",
      "destination_type": "queue",
      "routing_key": "#",
      "arguments": {}
    }
  ]
}
